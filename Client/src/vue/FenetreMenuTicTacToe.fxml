<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="380.0" prefWidth="536.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlleur.TicTacToeMenuControleur">
   <opaqueInsets>
      <Insets top="50.0" />
   </opaqueInsets>
   <children>
      <Label contentDisplay="CENTER" layoutX="62.0" layoutY="53.0" prefHeight="81.0" prefWidth="472.0" text="Veuillez choisir un mode de jeu Tic Tac Toe " textAlignment="CENTER" textFill="#11bd6a">
         <font>
            <Font name="Arial Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="soloBtn" layoutX="214.0" layoutY="177.0" mnemonicParsing="false" onAction="#VersTicTacToe1" prefHeight="39.0" prefWidth="110.0" style="-fx-background-color: lightgreen;" text="Solo">
         <font>
            <Font name="Arial Bold" size="13.0" />
         </font></Button>
      <Button fx:id="MulBtn" layoutX="214.0" layoutY="245.0" mnemonicParsing="false" onAction="#VersTicTacToe2" prefHeight="39.0" prefWidth="110.0" style="-fx-background-color: lightgreen;" text="Multijoueur">
         <font>
            <Font name="Arial Bold" size="13.0" />
         </font></Button>
      <Button fx:id="quitterBtn" layoutX="26.0" layoutY="323.0" mnemonicParsing="false" onAction="#Quitter" style="-fx-background-color: blue;" text="quitter" textFill="WHITE">
         <font>
            <Font name="Arial Bold" size="12.0" />
         </font></Button>
   </children>
</AnchorPane>
